  `location_az` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `location_ru` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `category_id` int(11) NOT NULL,
  `youtube` varchar(500) COLLATE utf8_unicode_ci DEFAULT NULL,
  `project_date` date DEFAULT NULL,
  `client` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `seo_title_az` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `seo_title_ru` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `seo_keywords_az` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
